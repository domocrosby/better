extends layout
block content
      #nav
        #brand
          img(src='images/logo1.svg')#logo
          p#title Card Wave
      #main
        #div
          a.btn.btn-primary(href="/auth/facebook")
            span.fa.fa-facebook Facebook #{user}
          if user
            img.image(src=user.facebook.picture)
          else
            p No Username!
          
      #tab
        .icon#addBtn
          span.glyphicon.glyphicon-plus
        .icon#searchBtn
          span.glyphicon.glyphicon-search
      #rail
     
      

        

